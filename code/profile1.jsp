      <h1>User Registration Form</h1>
      <div id="respond"> 
        <form name="f" enctype="multipart/form-data" action="insertprofile.jsp" method="post">
          <p> 
            <input type="text" name="name" id="name" value="" size="22" />
            <label for="name"><small>Name (required)</small></label>
          </p>
          <p> 
            <input type="text" name="birth" id="birth" value="" size="22" />
            <label for="name"><small>Date of Birth (required)</small></label>
          </p>
          <p> 
            <input type="text" name="gender" id="gender" value="" size="22" />
            <label for="name"><small>Gender (required)</small></label>
          </p>
          <p> 
            <input type="text" name="email" id="email" value="" size="22" />
            <label for="name"><small>Email Id (required)</small></label>
          </p>
          <p> 
            <input type="text" name="password" id="password" value="" size="22" />
            <label for="name"><small>Password (required)</small></label>
          </p>
          <p> 
            <textarea name="comment" id="comment" cols="10" rows="10" placeholder="Comment (required)"></textarea>
            <label for="comment" style="display:none;"><small>Comment (required)</small></label>
          </p>
          <p> 
            <input type="file" name="image" id="image" value="" size="22" />
            <label for="image"><small>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Your Picture</small></label>
          </p>
          <p> 
            <input name="submit" type="submit" id="submit" value="Submit Form" />
            &nbsp; 
            <input name="reset" type="reset" id="reset" tabindex="5" value="Reset Form" />
          </p>
        </form>
